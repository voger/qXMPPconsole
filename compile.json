{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "bundle": {
        "include": "qx.*"
      }
    },
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": ["en"],
  "environment": {
    "qx.icontheme": "Tango"
  },
  "include": ["qx.module.Template", "qx.module.Core"],
  "applications": [
    {
      "class": "qmc.Application",
      "theme": "qmc.theme.Theme",
      "name": "qmc",
      "bootPath": "source/boot"
    }
  ]
}
